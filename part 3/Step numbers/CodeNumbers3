def power(num,exp):
    result = 1
    for z in range(exp):
        result = result * num
    
    return result

def superpower(a,b):
    return power(a,b) + power(b,a)
    z = power(b,a)
    y = power(a,b)
    result = (z+y)
    return result

x = int(input("Enter number "))
y = int(input("Enter another number "))
z = superpower(x,y)
print (z)