import csv


test_str = "GeeksforGeeks is best"
 
# printing original string
print("The original string is : " + test_str)
 
# Using split()
# Removing Initial word from string
res = test_str.split(' ', 1)[1]
 
# printing result
print("The string after omitting first word is : " + str(res))

